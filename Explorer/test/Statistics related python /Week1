        "room_temp = 25\n",
        "\n",
        "if room_temp < 10:\n",
        "    print(\"Turn on the big heaters\")\n",
        "elif room_temp < 20:\n",
        "    print(\"Turn on the small heaters\")\n",
        "else:\n",
        "    print(\"Turn on the fans\")\n",
        "\n",
        "print(\"Conditional block complete\") #和条件语句无关，是一直会在"
      ]
