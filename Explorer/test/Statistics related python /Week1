        "os.listdir()\n",
        "import os\n",
        "\n",
        "# 列出当前工作目录中的文件列表\n",
        "file_list = os.listdir()\n",
        "\n",
        "# 遍历文件列表，找到目标文件的路径\n",
        "target_file = \"world_population.csv\"  # 将 \"your_file_name.csv\" 替换为您要查找的文件名\n",
        "target_file_path = None\n",
        "\n",
        "for file in file_list:\n",
        "    if file == target_file:\n",
        "        target_file_path = os.path.abspath(file)\n",
        "        break\n",
        "\n",
        "if target_file_path:\n",
        "    print(\"目标文件的完整路径：\", target_file_path)\n",
